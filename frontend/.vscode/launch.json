{
  "configurations": [
    {
      "name": "Attach Chrome localhost:4200",
      "port": 9222,
      "request": "attach",
      "type": "chrome",
      "urlFilter": "http://localhost:4200/*",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Launch Chrome localhost:4200",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}"
    }
  ],
  "version": "0.2.0"
}
